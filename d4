#include <stdio.h>

int removeElement(int* nums, int numsSize, int val) 
   {
    int k = 0; 
    for (int i = 0; i < numsSize; i++) {
        if (nums[i] != val) {
            nums[k] = nums[i];
            k++;
        }
    }
    return k;
}

void printArray(int* nums, int k, int numsSize)
   {
    printf("[");
    for (int i = 0; i < numsSize; i++) {
        if (i < k) printf("%d", nums[i]);
        else printf("_");
        if (i < numsSize - 1) printf(",");
    }
    printf("]\n");
}

int main()
   {
    int nums1[] = {3, 2, 2, 3};
    int val1 = 3;
    int k1 = removeElement(nums1, 4, val1);
    printf("k = %d, nums = ", k1);
    printArray(nums1, k1, 4);
    int nums2[] = {0, 1, 2, 2, 3, 0, 4, 2};
    int val2 = 2;
    int k2 = removeElement(nums2, 8, val2);
    printf("k = %d, nums = ", k2);
    printArray(nums2, k2, 8);
    
    return 0;
}
